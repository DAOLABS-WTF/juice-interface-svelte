		"@types/cookie": "^0.4.1",
		"@types/d3": "^7.1.0",
		"@types/dompurify": "^2.3.3",
		"@types/marked": "^4.0.3",
		"@web3-onboard/core": "file:./libraries/@web3-onboard/core",
