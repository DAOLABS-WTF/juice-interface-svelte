		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"rollup-plugin-polyfill-node": "^0.9.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte-feather-icons": "^4.0.0",
		"svelte-i18n": "^3.4.0",
