		"@web3-onboard/injected-wallets": "^2.0.6",
		"@web3-onboard/ledger": "^2.0.4",
		"@web3-onboard/trezor": "^2.0.5",
		"@web3-onboard/walletconnect": "^2.0.1",
		"autoprefixer": "^10.4.7",
